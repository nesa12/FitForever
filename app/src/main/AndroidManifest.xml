<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fitforever">

    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FitForever">


        <activity
            android:name=".WelcomePage"
            android:label="WELCOME TO FitForever"
            android:parentActivityName=".StartFitForever" />

        <activity android:name=".MyProfile" />
        <activity android:name=".UpdateWeight" />
        <activity android:name=".BMICalculator" />
        <activity android:name=".BMIResult" />
        <activity android:name=".SetReminder" />
        <activity android:name=".AddActivity" />
        <activity android:name=".RateUs" />


        <activity
            android:name=".StartFitForever"
            android:exported="true"
            android:label="FitForever">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>



        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>