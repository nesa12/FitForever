<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fitforever">

    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.FitForever">



        <activity
            android:name="com.example.fitforever.WaterIntakeCalculator"
            android:label="Daily Water Intake Calculator"
            android:parentActivityName="com.example.fitforever.WelcomeToTimetablePage" />
        <activity
            android:name="com.example.fitforever.UpdateTimetable"
            android:label="UPDATE DETAILS"
            android:parentActivityName="com.example.fitforever.Timetable" />
        <activity
            android:name="com.example.fitforever.Adddetails"
            android:label="ADD DETAILS"
            android:parentActivityName="com.example.fitforever.Timetable" />
        <activity
            android:name="com.example.fitforever.Timetable"
            android:label="Progress Plan"
            android:parentActivityName="com.example.fitforever.WelcomeToTimetablePage" />



        <activity android:name="com.example.fitforever.WelcomeToTimetablePage"
            android:label="Progress Shedule"
            />


        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>